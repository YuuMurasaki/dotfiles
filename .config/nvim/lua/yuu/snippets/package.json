{
    "name": "friendly-snippets",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": [
                    "plaintext",
                    "markdown",
                    "global",
                    "all"
                ],
                "path": "./global.json"
            },
            {
                "language": "sh",
                "path": "./shell.json"
            },
            {
                "language": "markdown",
                "path": "./markdown.json"
            }
        ]
    }
}
